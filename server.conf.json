{
	"listen": [
		{
			"type": "websocket",
			"port": 13900
		},
		{
			"type": "tcp",
			"port": 13902
		}
	],
	"logging": "info",
	"nodes": {
		"default": "Exchange",
		"readonly": {
			"type": "Exchange",
			"options": {
				"readonly": true
			}
		},
		"state": {
			"type": "TopicStore",
			"options": {
				"persistent": true
			}
		},
		"ping": "PingResponder",
		"blib": {
			"type": "TestSource",
			"options": {
				"topic": "blib",
				"interval": 5000
			}
		},
		"test": {
			"type": "Queue",
			"options": {
				"capacity": 5,
				"pattern": ["test:*", "blib"],
				"persistent": true
			}
		}
	},
	"bindings": [
		{ "from": "blib", "to": "test" },
		{ "from": "default", "to": "readonly" }
	],
	"storage": "./storage/"
}
